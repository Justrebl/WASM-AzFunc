{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[3.*, 4.0.0)"
  },
  "customHandler": {
    "description": {
      "defaultExecutablePath": "spin.exe",
      "workingDirectory": "",
      "arguments": [
        "up",
        "--listen",
        "127.0.0.1:%FUNCTIONS_CUSTOMHANDLER_PORT%"
      ]
    },
		"enableForwardingHttpRequest":true
  }
}